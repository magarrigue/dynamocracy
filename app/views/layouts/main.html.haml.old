!!! html 5
%html
  %head
    %title= display_title
    %meta(http-equiv="content-type" content="text/html; charset=utf-8")
    = js_includes
    = stylesheet_link_tag "application"
  %body
    .wrap_header
      %header
        %h1= link_to_t 'Home', root_path

    .wrap_menu
      .menu
        .wrap_content
      .content
        #error_div_id.flash.error(style="display:none")
        #notice_div_id.flash.notice(style="display:none")
        = yield
        %br
    .wrap_footer
      .footer
        %p.footer-nav
          <a>About</a> | <a>FAQ</a> | <a>Blog</a> | <a>Contact</a> | <a>Terms</a> | <a>Privacy</a> | <a>Support</a>
        %p.copyright Copyright Â© 2009-2010 Dynamocracy.org
        
